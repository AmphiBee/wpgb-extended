{
  "name": "amphibee/wpgb-extended",
  "description": "Register Gridbuilder ᵂᴾ template and (soon) facets with object oriented PHP",
  "type": "wordpress-muplugin",
  "keywords": [
    "wpgb",
    "wp",
    "grid",
    "builder",
    "amphibee",
    "wordpress"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Olivier Gorzalka",
      "email": "olivier@amphibee.fr"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "amphibee/hookable": "dev-main"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.6"
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "AmphiBee\\WpgbExtended\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
